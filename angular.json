                },
                "@schematics/angular:interceptor": {
                    "skipTests": true
                },
                "@schematics/angular:module": {
                    "skipTests": true
                },
                "@schematics/angular:pipe": {
                    "skipTests": true
                },
                "@schematics/angular:service": {
                    "skipTests": true
                }
            },
            "root": "projects/application-two",
            "sourceRoot": "projects/application-two/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-builders/custom-webpack:browser",
                    "options": {
                        "outputPath": "dist/application-two",
                        "index": "projects/application-two/src/index.html",
                        "main": "projects/application-two/src/main.ts",
                        "polyfills": "projects/application-two/src/polyfills.ts",
                        "tsConfig": "projects/application-two/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "projects/application-two/src/favicon.ico",
                            "projects/application-two/src/assets"
                        ],
                        "styles": [
                            "projects/application-two/src/styles.scss"
                        ],
                        "scripts": [],
                        "customWebpackConfig": {
                            "path": "./projects/application-two/custom-webpack.config.js"
                        }
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "projects/application-two/src/environments/environment.ts",
                                    "with": "projects/application-two/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-builders/custom-webpack:dev-server",
                    "options": {
                        "browserTarget": "application-two:build",
                        "port": 9003
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "application-two:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "application-two:build"
                    }
                }
            }
        }
    },
    "defaultProject": "qiankun-learn"
}